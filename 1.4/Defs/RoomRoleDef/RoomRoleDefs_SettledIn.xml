  <!-- ElectricalRoom -->
  <RoomRoleDef Class="DanielRenner.SettledIn.SettledInDynamicRoomRoleDef" Parent="SettledInRoomRoleDynamic">
    <defName>ElectricalRoom</defName>
    <label>Electrical Room</label>
    <description>A cabinet containing electrification components like fuses, batteries and power switches.</description>
    <workerClass>DanielRenner.SettledIn.RoomRoleWorker_Electrical_Room</workerClass>
    <perfectDefs>
      <li>Building_RTMakeshiftFuse</li>
      <li>VFE_SmallBattery</li>
      <li>VFE_LargeBattery</li>
      <li>Battery</li>
      <li>PowerSwitch</li>
    </perfectDefs>
    <positiveDefs>
    </positiveDefs>
    <negativeDefs>
    </negativeDefs>
    <forbiddenDefs>
      <li>Bed</li>
      <li>DoubleBed</li>
      <li>SleepingSpot</li>
